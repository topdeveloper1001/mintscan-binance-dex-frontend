{
  "name": "mintscan-binance",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "REACT_APP_BUILD_ENV=prod && react-scripts start",
    "dev": "REACT_APP_BUILD_ENV=dev && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "delete-maps": "yarn run delete-references-to-map-files && yarn run delete-map-files",
    "delete-map-files": "find ./build -name '*.map' -delete",
    "delete-references-to-map-files": "find ./build -regex '.*\\.\\(js\\|css\\)' -exec sed -i -E '\\/[\\*\\/]#\\ssourceMappingURL=main(\\.[0-9a-f]+)?\\.(css|js)\\.map(\\*\\/)?/g' {} +",
    "prettify": "prettier --write src/**/*"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.1",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/styles": "^4.9.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/lodash": "^4.14.149",
    "@types/react-router-dom": "^5.1.3",
    "axios": "^0.19.2",
    "base64-js": "^1.3.1",
    "big.js": "^5.2.2",
    "chalk": "^3.0.0",
    "classnames": "^2.2.6",
    "fast-deep-equal": "^3.1.1",
    "highcharts": "^8.0.0",
    "highcharts-react-official": "^2.2.2",
    "husky": "^4.2.1",
    "include-media": "^1.4.9",
    "is-empty": "^1.2.0",
    "js-sha256": "^0.9.0",
    "linkifyjs": "^2.1.9",
    "lint-staged": "^10.0.7",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "query-string": "^6.10.1",
    "react": "^16.12.0",
    "react-app-polyfill": "^1.0.6",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dev-utils": "^10.1.0",
    "react-dom": "^16.12.0",
    "react-ga": "^2.7.0",
    "react-html-parser": "^2.0.2",
    "react-loaders": "^3.0.1",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "react-skeleton-loader": "^1.0.4",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-pender": "^2.0.12",
    "source-map-explorer": "^2.2.2"
  },
  "devDependencies": {},
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "compilerOptions": {
    "baseUrl": "src"
  },
  "include": ["src"]
}
